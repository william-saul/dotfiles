[list]
order = numeric
reverse = yes
fsmon = yes

[gallery]
size = 450
preload = no
cache = 100

[keys.viewer]
F1 = help
gg = first_file
Shift+g = last_file
h = prev_file
l = next_file
Shift+k = zoom +10
Shift+j  = zoom -10

[keys.slideshow]
F1 = help
gg = first_file
Shift+g = last_file
h = step_left
l = step_right
k = step_up
j = step_down
u = page_up
d = page_down

[keys.gallery]
F1 = help
gg = first_file
Shift+g = last_file
h = step_left
l = step_right
k = step_up
j = step_down
u = page_up
d = page_down
